<div class="container">
    <form class="form-horizontal">
        <fieldset>

            <!-- Form Name -->
            <legend>Account Settings</legend>

            <!-- Text input-->
            <div class="form-group">
                <label class="col-md-4 control-label" for="textinput">Change Email ID</label>
                <div class="col-md-4">
                    <input id="textinput" name="textinput" type="text" placeholder="your current email " class="form-control input-md">
                </div>
                <br>
                <button class="btn btn-primary btn-email" type="submit">Change Email</button>
            </div>

            <!-- Password input-->
            <div class="form-group">
                <label class="col-md-4 control-label" for="passwordinput">new password</label>
                <div class="col-md-4">
                    <input id="passwordinput" name="passwordinput" type="password" placeholder="new password" class="form-control input-md">
                </div>
            </div>

            <!-- Password input-->
            <div class="form-group">
                <label class="col-md-4 control-label" for="passwordinput">repeat password</label>
                <div class="col-md-4">
                    <input id="passwordinput" name="passwordinput" type="password" placeholder="repeat password" class="form-control input-md">
                    <br>
                    <button class="btn btn-primary btn-passowrd" type="submit">Change Password</button>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="form-group">
                <label class="col-md-4 control-label" for="selectbasic">Set Security Question</label>
                <div class="col-md-4">
                    <select id="selectbasic" name="selectbasic" class="form-control">
                        <option value="1">Where is your hometown?</option>
                        <option value="2">What is your favorite Dinosaur?</option>
                        <option value="3">Do you prefer Chicken on your Waffles?</option>
                    </select>
                </div>
                <label class="col-md-4 control-label" for="passwordinput">repeat password</label>
                <div class="col-md-4">
                    <input id="securityanswer" name="securityanswer" type="security" placeholder="security question answer" class="form-control input-md">
                </div>
                <br>
                <button class="btn btn-primary btn-question" type="submit">Set Security Question</button>
            </div>

            <!-- Button -->
            <div class="form-group">
                <label class="col-md-4 control-label" for="singlebutton">view account status</label>
                <div class="col-md-4">
                    <button id="singlebutton" name="singlebutton" class="btn btn-success">view</button>
                </div>
            </div>

            <!-- Button -->
            <div class="form-group">
                <label class="col-md-4 control-label" for="singlebutton">Delete my account</label>
                <div class="col-md-4">
                    <button id="singlebutton" name="singlebutton" class="btn btn-danger">Delete</button>
                </div>
            </div>
            <!-- Select Multiple -->
            <div class="form-group">
                <label class="col-md-4 control-label" for="selectmultiple">Languages</label>
                <div class="col-md-4">
                    <select id="selectmultiple" name="selectmultiple" class="form-control" multiple="multiple">
                        <option value="1">Persian</option>
                        <option value="2">English</option>
                        <option value="3">Arabic</option>
                        <option value="2">Spanish</option>
                        <option value="3">Japanese</option>
                        <option value="2">Sri Lankan</option>
                        <option value="3">Chinese</option>
                    </select>
                </div>
            </div>
            <div class="from-group row">
                <button class="btn btn-primary btn-language" type="submit">Change Language</button>
                <br>
            </div>

        </fieldset>
    </form>

</div>
</body>

