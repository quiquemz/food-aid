    </div>
</nav>

<div class="container">
    <div class="card card-container">
        <img id="img" class="profile-img-card" src="images/logo.png" />
        <p id="profile-name" class="profile-name-card"></p>
        <form class="form-signin" action="login" method="post">
            <span class="reauth-email"></span>
            <input type="email" id="input-email" name="email" class="form-control" placeholder="Email address" required autofocus style=""/>
            <input type="password" id="input-password" name="password" class="form-control" placeholder="Password" required/>
            <a href="#" class="forgot-password">
                Forgot password?
            </a>
            <button class="btn btn-lg btn-primary btn-block btn-signin" type="submit">Sign in</button>
        </form><!-- /form -->
        <a href="/register" class="btn btn-lg btn-success btn-block btn-register" type="submit">
            Register
        </a>
    </div><!-- /card-container -->
</div><!-- /container -->

